<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Kind</key>
    <string>Xcode.Xcode3.ProjectTemplateUnitKind</string>
    <key>Identifier</key>
    <string>com.anas.dt.unit.cocoaTouchApplicationBase_swift</string>
    <key>Ancestors</key>
    <array>
        <string>com.anas.dt.unit.applicationBase</string>
        <string>com.anas.dt.unit.iosBase</string>
    </array>
    <key>Targets</key>
    <array>
        <dict>
            <key>TargetIdentifier</key>
            <string>com.apple.dt.cocoaTouchApplicationTarget</string>
            <key>SharedSettings</key>
            <dict>
                <key>ASSETCATALOG_COMPILER_APPICON_NAME</key>
                <string>AppIcon</string>
                <key>LD_RUNPATH_SEARCH_PATHS</key>
                <string>$(inherited) @executable_path/Frameworks</string>
            </dict>
        </dict>
    </array>
    <key>Options</key>
    <array>
    
        <dict>
            <!-- Set swift version -->
            <key>Identifier</key>
            <string>swiftVersions</string>
            <key>Name</key>
            <string>Swift:</string>
            <key>Description</key>
            <string>Which Swift version to use</string>
            <key>SortOrder</key>
            <integer>1</integer>
            <key>Type</key>
            <string>popup</string>
            <key>Default</key>
            <string>4.0</string>
            <key>NotPersisted</key>
            <false/>
            <key>Values</key>
            <array>
                <string>4.0</string>
                <string>3.2</string>
                <string>3.1</string>
            </array>
            <key>Units</key>
            <dict>
                <key>3.1</key>
                <dict>     
                    <key>Project</key>
                    <dict>
                        <key>SharedSettings</key>
                        <dict>
                            <key>SWIFT_VERSION</key>
                            <string>3.1</string>
                        </dict>
                    </dict>
                </dict>

                <key>3.2</key>
                <dict>          
                    <key>Project</key>
                    <dict>
                        <key>SharedSettings</key>
                        <dict>
                            <key>SWIFT_VERSION</key>
                            <string>3.2</string>
                        </dict>
                    </dict>
                </dict>

                <key>4.0</key>
                <dict>
                    <key>Project</key>
                    <dict>
                        <key>SharedSettings</key>
                        <dict>
                            <key>SWIFT_VERSION</key>
                            <string>4.0</string>
                        </dict>
                    </dict>
                </dict>
            </dict>
        </dict>

        <dict>
            <key>Identifier</key>
            <string>universalDeviceFamily</string>
            <key>Name</key>
            <string>Devices:</string>
            <key>Description</key>
            <string>Which device family to create a project for</string>
            <key>SortOrder</key>
            <integer>2</integer>
            <key>Type</key>
            <string>popup</string>
            <key>Default</key>
            <string>Universal</string>
            <key>Values</key>
            <array>
                <string>Universal</string>
                <string>iPhone</string>
                <string>iPad</string>
            </array>
            <key>Units</key>
            <dict>
                <key>iPhone</key>
                <dict>
                    <key>Nodes</key>
                    <array>
                        <string>Info.plist:UISupportedInterfaceOrientations~iPhone</string>
                    </array>
                    <key>Definitions</key>
                    <dict>
                        <key>Assets.xcassets</key>
                        <dict>
                            <key>Path</key>
                            <string>Images-iPhone.xcassets</string>
                            <key>SortOrder</key>
                            <integer>100</integer>
                        </dict>
                    </dict>
                </dict>
                <key>iPad</key>
                <dict>
                    <key>Project</key>
                    <dict>
                        <key>SharedSettings</key>
                        <dict>
                            <key>TARGETED_DEVICE_FAMILY</key>
                            <string>2</string>
                        </dict>
                    </dict>
                    <key>Nodes</key>
                    <array>
                        <string>Info.plist:UISupportedInterfaceOrientations~iPad</string>
                    </array>
                    <key>Definitions</key>
                    <dict>
                        <key>Assets.xcassets</key>
                        <dict>
                            <key>Path</key>
                            <string>Images-iPad.xcassets</string>
                            <key>SortOrder</key>
                            <integer>100</integer>
                        </dict>
                    </dict>
                </dict>
                <key>Universal</key>
                <dict>
                    <key>Project</key>
                    <dict>
                        <key>SharedSettings</key>
                        <dict>
                            <key>TARGETED_DEVICE_FAMILY</key>
                            <string>1,2</string>
                        </dict>
                    </dict>
                    <key>Nodes</key>
                    <array>
                        <string>Info.plist:UISupportedInterfaceOrientations~iPhone</string>
                        <string>Info.plist:UISupportedInterfaceOrientations~iPad</string>
                    </array>
                    <key>Definitions</key>
                    <dict>
                        <key>Assets.xcassets</key>
                        <dict>
                            <key>Path</key>
                            <string>Images-Universal.xcassets</string>
                            <key>SortOrder</key>
                            <integer>100</integer>
                        </dict>
                    </dict>
                </dict>
            </dict>
        </dict>
        <dict>
            <key>Identifier</key>
            <string>hasUnitTests</string>
            <key>Name</key>
            <string>Include Unit Tests</string>
            <key>NotPersisted</key>
            <false/>
            <key>SortOrder</key>
            <integer>100</integer>
            <key>Type</key>
            <string>checkbox</string>
            <key>Default</key>
            <string>true</string>
            <key>Units</key>
            <dict>
                <key>true</key>
                <dict>
					<key>Nodes</key>
					<array>
						<string>../Podfile:unitTest</string>
					</array>

                    <key>Components</key>
                    <array>
                        <dict>
                            <key>Identifier</key>
                            <string>com.apple.dt.unit.cocoaTouchApplicationUnitTestBundle</string>
                            <key>Name</key>
                            <string>___PACKAGENAME___Tests</string>
                        </dict>
                    </array>
                </dict>
            </dict>
        </dict>
        <dict>
            <key>Identifier</key>
            <string>hasUITests</string>
            <key>Name</key>
            <string>Include UI Tests</string>
            <key>NotPersisted</key>
            <false/>
            <key>SortOrder</key>
            <integer>101</integer>
            <key>Type</key>
            <string>checkbox</string>
            <key>Default</key>
            <string>true</string>
            <key>Units</key>
            <dict>
                <key>true</key>
                <dict>
                	<key>Nodes</key>
					<array>
						<string>../Podfile:unitTest:UITest</string>
					</array>

                    <key>Components</key>
                    <array>
                        <dict>
                            <key>Identifier</key>
                            <string>com.apple.dt.unit.cocoaTouchApplicationUITestBundle</string>
                            <key>Name</key>
                            <string>___PACKAGENAME___UITests</string>
                        </dict>
                    </array>
                </dict>
            </dict>
        </dict>
    </array>


    <key>Nodes</key>
    <array>
        <!-- cocoa pods -->
        <string>../Podfile</string>
        
        <string>Info.plist:iPhone</string>
        <string>Info.plist:UIRequiredDeviceCapabilities:base</string>
        <string>Info.plist:LaunchScreen</string>
        <string>Assets.xcassets</string>
        <string>Base.lproj/LaunchScreen.storyboard</string>
    </array>


    <key>Definitions</key>
    <dict>

        <key>../Podfile</key>
		<dict>
            <!-- podfile definition -->
            <key>Beginning</key>
			<string>
source &apos;https://github.com/CocoaPods/Specs.git&apos;
# add your sources here

# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target &apos;___PACKAGENAME___&apos; do
    platform :ios, '9.0'
    use_frameworks!
  
    # Pods for ___PACKAGENAME___
end
</string>
        <key>End</key>
        <!-- end of pod file (nothing to put here -->
        <string>
        </string>

					
            <key>TargetIndices</key>
			<array/>
            <key>Group</key>
            <string>no_xCode</string>
			<key>Path</key>
			<string>Podfile</string>
	    </dict>

        <!-- testing pods -->
        <key>../Podfile:unitTest</key>
        <dict>
            <key>Beginning</key>
            <string>target &apos;___PACKAGENAME___Tests&apos; do
    platform :ios, '9.0'
    use_frameworks!
</string>
            <key>End</key>
            <!-- pod file end keyword -->
            <string>end</string>
            
            <key>Indent</key>
			<integer>1</integer>
        </dict>

        <key>../Podfile:unitTest:UITest</key>
        <string>target &apos;___PACKAGENAME___UITests&apos; do
    # UI test pods
end</string>
        
        <key>Info.plist:iPhone</key>
        <string><![CDATA[<key>LSRequiresIPhoneOS</key>
<true/>
]]></string>
        <key>Info.plist:UIRequiredDeviceCapabilities</key>
        <dict>
            <key>Beginning</key>
            <string><![CDATA[<key>UIRequiredDeviceCapabilities</key>
<array>]]></string>
            <key>End</key>
            <string><![CDATA[</array>]]></string>
            <key>Indent</key>
            <integer>1</integer>
        </dict>
        <key>Info.plist:UIRequiredDeviceCapabilities:base</key>
        <string><![CDATA[<string>armv7</string>]]></string>
        <key>Info.plist:statusBarTintForNavBar</key>
        <string><![CDATA[<key>UIStatusBarTintParameters</key>
<dict>
    <key>UINavigationBar</key>
    <dict>
        <key>Style</key>
        <string>UIBarStyleDefault</string>
        <key>Translucent</key>
        <false/>
    </dict>
</dict>
]]></string>
        <key>Info.plist:LaunchScreen</key>
        <string><![CDATA[<key>UILaunchStoryboardName</key>
<string>LaunchScreen</string>
]]></string>
        <key>Base.lproj/LaunchScreen.storyboard</key>
        <dict>
            <key>Path</key>
            <string>LaunchScreen.storyboard</string>
            <key>SortOrder</key>
            <integer>101</integer>
        </dict>
</dict>
</dict>
</plist>
